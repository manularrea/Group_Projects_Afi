library(ggplot2)
library(dplyr)
library(ggridges) # Para gráficos de densidad de direferentes niveles
drugs <- read.csv("../data/drug_consumption_clean_r.csv", check.names = FALSE)
# Gráfico de impulsividad por consumo de alcohol.
impulsive_alcohol <- drugs %>%
select(impulsive, alcohol, age)
consumo <- c("Nunca ha consumido",
"Más de una decada",
"Última década",
"Último año",
"Último mes",
"Última semana",
"Último día")
# Se hace la visualización por la poblacion agrupada de 18-24 años solamente, porque los demas rangos no reflejaron grandes variaciones
ggplot(impulsive_alcohol[impulsive_alcohol$age  == "18-24",],
aes(x = impulsive, y = as.factor(alcohol), fill = as.factor(alcohol))) +
geom_density_ridges(alpha = 0.5) +
labs(title ="Distribución de la impulsividad por fecha del último consumo de alcohol",
x ="Impulsividad", y = "Última fecha de consumo") +
theme_minimal() +
theme(legend.position = "none") +
scale_y_discrete(labels = consumo)
